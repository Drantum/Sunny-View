<?xml version="1.0" encoding="utf-8"?>
<s:Group xmlns:fx="http://ns.adobe.com/mxml/2009"
		 xmlns:s="library://ns.adobe.com/flex/spark"
		 xmlns:mx="library://ns.adobe.com/flex/mx"
		 width="250" height="200">
	<fx:Declarations>
		<!-- Platzieren Sie nichtvisuelle Elemente (z.Â B. Dienste, Wertobjekte) hier -->
	</fx:Declarations>
	
	<fx:Script>
		<![CDATA[
			[Bindable]
			public var wechselrichterName:String;
			[Bindable]
			public var pac:String;
			[Bindable]
			public var eHeute:String;
			[Bindable]
			public var eTotal:String;
			
		]]>
	</fx:Script>
	
	<s:Panel x="1" y="1" width="250" height="200" title="{wechselrichterName}">
		<s:Label x="10" y="10" text="Pac"/>
		<s:Label x="10" y="30" text="E-heute"/>
